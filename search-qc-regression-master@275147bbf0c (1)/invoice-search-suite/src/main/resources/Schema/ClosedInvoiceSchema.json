    "resultType": {
      "$id": "#/properties/resultType",
      "type": "string",
      "title": "The Resulttype Schema",
      "default": "",
      "examples": [
        "invoiceDetailsReport"
      ],
      "pattern": "^(.*)$"
    },
    "requestBody": {
      "$id": "#/properties/requestBody",
      "type": "object",
      "title": "The Requestbody Schema",
      "required": [
        "parameters"
      ],
      "properties": {
        "parameters": {
          "$id": "#/properties/requestBody/properties/parameters",
          "type": "object",
          "title": "The Parameters Schema",
          "required": [
            "buCode",
            "endDate",
            "startDate"
          ],
          "properties": {
            "buCode": {
              "$id": "#/properties/requestBody/properties/parameters/properties/buCode",
              "type": "array",
              "title": "The Bucode Schema",
              "items": {
                "$id": "#/properties/requestBody/properties/parameters/properties/buCode/items",
                "type": "string",
                "title": "The Items Schema",
                "default": "",
                "examples": [
                  "OHTO2"
                ],
                "pattern": "^(.*)$"
              }
            },
            "endDate": {
              "$id": "#/properties/requestBody/properties/parameters/properties/endDate",
