{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "responseHeader",
    "responseBody",
    "resultType",
    "requestBody"
  ],
  "properties": {
    "responseHeader": {
      "$id": "#/properties/responseHeader",
      "type": "object",
      "title": "The Responseheader Schema",
      "required": [
        "status"
      ],
      "properties": {
        "status": {
          "$id": "#/properties/responseHeader/properties/status",
          "type": "string",
          "title": "The Status Schema",
          "default": "",
          "examples": [
            "OK"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "responseBody": {
      "$id": "#/properties/responseBody",
      "type": "object",
      "title": "The Responsebody Schema",      
      "properties": {
        "aggregations": {
          "$id": "#/properties/responseBody/properties/aggregations",
          "type": "array",
          "title": "The Aggregations Schema",
          "items": {
            "$id": "#/properties/responseBody/properties/aggregations/items",
            "type": "object",
            "title": "The Items Schema",           
            "properties": {
              "name": {
                "$id": "#/properties/responseBody/properties/aggregations/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "treeBranch"
                ],
                "pattern": "^(.*)$"
              },
              "docCount": {
                "$id": "#/properties/responseBody/properties/aggregations/items/properties/docCount",
                "type": "integer",
                "title": "The Doccount Schema",
                "default": 0,
                "examples": [
                  1
                ]
              },
              "buckets": {
                "$id": "#/properties/responseBody/properties/aggregations/items/properties/buckets",
                "type": "array",
                "title": "The Buckets Schema",
                "items": {
                  "$id": "#/properties/responseBody/properties/aggregations/items/properties/buckets/items",
                  "type": "object",
                  "title": "The Items Schema",                 
                  "properties": {
                    "name": {
                      "$id": "#/properties/responseBody/properties/aggregations/items/properties/buckets/items/properties/name",
                      "type": "string",
                      "title": "The Name Schema",
                      "default": "",
                      "examples": [
                        "CORPORATE"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "docCount": {
                      "$id": "#/properties/responseBody/properties/aggregations/items/properties/buckets/items/properties/docCount",
                      "type": "integer",
                      "title": "The Doccount Schema",
                      "default": 0,
                      "examples": [
                        1
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "data": {
          "$id": "#/properties/responseBody/properties/data",
          "type": "array",
          "title": "The Data Schema",
          "items": {
            "$id": "#/properties/responseBody/properties/data/items",
            "type": "object",
            "title": "The Items Schema",           
            "properties": {
              "approverId": {
                "$id": "#/properties/responseBody/properties/data/items/properties/approverId",
                "type": "string",
                "title": "The Approverid Schema",
                "default": "",
                "examples": [
                  "0001940"
                ],
                "pattern": "^(.*)$"
              },
              "departmentId": {
                "$id": "#/properties/responseBody/properties/data/items/properties/departmentId",
                "type": "string",
                "title": "The Departmentid Schema",
                "default": "",
                "examples": [
                  "04ITQL"
                ],
                "pattern": "^(.*)$"
              },
              "districtManagerIdName": {
                "$id": "#/properties/responseBody/properties/data/items/properties/districtManagerIdName",
                "type": "string",
                "title": "The Districtmanageridname Schema",
                "default": "",
                "examples": [
                  "null:null"
                ],
                "pattern": "^(.*)$"
              },
              "email": {
                "$id": "#/properties/responseBody/properties/data/items/properties/email",
                "type": "string",
                "title": "The Email Schema",
                "default": "",
                "examples": [
                  "ayucuis@fastenal.com"
                ],
                "pattern": "^(.*)$"
              },
              "employeeId": {
                "$id": "#/properties/responseBody/properties/data/items/properties/employeeId",
                "type": "string",
                "title": "The Employeeid Schema",
                "default": "",
                "examples": [
                  "0096881"
                ],
                "pattern": "^(.*)$"
              },
              "employeeStatusCode": {
                "$id": "#/properties/responseBody/properties/data/items/properties/employeeStatusCode",
                "type": "string",
                "title": "The Employeestatuscode Schema",
                "default": "",
                "examples": [
                  "A"
                ],
                "pattern": "^(.*)$"
              },
              "firstName": {
                "$id": "#/properties/responseBody/properties/data/items/properties/firstName",
                "type": "string",
                "title": "The Firstname Schema",
                "default": "",
                "examples": [
                  "Adam"
                ],
                "pattern": "^(.*)$"
              },
              "fullPartTime": {
                "$id": "#/properties/responseBody/properties/data/items/properties/fullPartTime",
                "type": "string",
                "title": "The Fullparttime Schema",
                "default": "",
                "examples": [
                  "F"
                ],
                "pattern": "^(.*)$"
              },
              "generalLedgerExpense": {
                "$id": "#/properties/responseBody/properties/data/items/properties/generalLedgerExpense",
                "type": "string",
                "title": "The Generalledgerexpense Schema",
                "default": "",
                "examples": [
                  "03MN170"
                ],
                "pattern": "^(.*)$"
              },
              "jobCode": {
                "$id": "#/properties/responseBody/properties/data/items/properties/jobCode",
                "type": "string",
                "title": "The Jobcode Schema",
                "default": "",
                "examples": [
                  "000911"
                ],
                "pattern": "^(.*)$"
              },
              "jobCodeTitle": {
                "$id": "#/properties/responseBody/properties/data/items/properties/jobCodeTitle",
                "type": "string",
                "title": "The Jobcodetitle Schema",
                "default": "",
                "examples": [
                  "000911:IT Quality Assurance Analyst"
                ],
                "pattern": "^(.*)$"
              },
              "jobTitle": {
                "$id": "#/properties/responseBody/properties/data/items/properties/jobTitle",
                "type": "string",
                "title": "The Jobtitle Schema",
                "default": "",
                "examples": [
                  "IT Quality Assurance Analyst"
                ],
                "pattern": "^(.*)$"
              },
              "lastHireDate": {
                "$id": "#/properties/responseBody/properties/data/items/properties/lastHireDate",
                "type": "string",
                "title": "The Lasthiredate Schema",
                "default": "",
                "examples": [
                  "2014-06-13"
                ],
                "pattern": "^(.*)$"
              },
              "lastName": {
                "$id": "#/properties/responseBody/properties/data/items/properties/lastName",
                "type": "string",
                "title": "The Lastname Schema",
                "default": "",
                "examples": [
                  "Yucuis"
                ],
                "pattern": "^(.*)$"
              },
              "location": {
                "$id": "#/properties/responseBody/properties/data/items/properties/location",
                "type": "string",
                "title": "The Location Schema",
                "default": "",
                "examples": [
                  "HEADI"
                ],
                "pattern": "^(.*)$"
              },
              "middleName": {
                "$id": "#/properties/responseBody/properties/data/items/properties/middleName",
                "type": "string",
                "title": "The Middlename Schema",
                "default": "",
                "examples": [
                  "James"
                ],
                "pattern": "^(.*)$"
              },
              "name": {
                "$id": "#/properties/responseBody/properties/data/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "Yucuis,Adam James"
                ],
                "pattern": "^(.*)$"
              },
              "preferredName": {
                "$id": "#/properties/responseBody/properties/data/items/properties/preferredName",
                "type": "string",
                "title": "The Preferredname Schema",
                "default": "",
                "examples": [
                  "Adam Yucuis"
                ],
                "pattern": "^(.*)$"
              },
              "regionManagerIdName": {
                "$id": "#/properties/responseBody/properties/data/items/properties/regionManagerIdName",
                "type": "string",
                "title": "The Regionmanageridname Schema",
                "default": "",
                "examples": [
                  "null:null"
                ],
                "pattern": "^(.*)$"
              },
              "supervisorId": {
                "$id": "#/properties/responseBody/properties/data/items/properties/supervisorId",
                "type": "string",
                "title": "The Supervisorid Schema",
                "default": "",
                "examples": [
                  "0001940"
                ],
                "pattern": "^(.*)$"
              },
              "supervisorIdName": {
                "$id": "#/properties/responseBody/properties/data/items/properties/supervisorIdName",
                "type": "string",
                "title": "The Supervisoridname Schema",
                "default": "",
                "examples": [
                  "0001940:Matt McNally"
                ],
                "pattern": "^(.*)$"
              },
              "supervisorName": {
                "$id": "#/properties/responseBody/properties/data/items/properties/supervisorName",
                "type": "string",
                "title": "The Supervisorname Schema",
                "default": "",
                "examples": [
                  "Matt McNally"
                ],
                "pattern": "^(.*)$"
              },
              "treeBranch": {
                "$id": "#/properties/responseBody/properties/data/items/properties/treeBranch",
                "type": "string",
                "title": "The Treebranch Schema",
                "default": "",
                "examples": [
                  "CORPORATE"
                ],
                "pattern": "^(.*)$"
              },
              "username": {
                "$id": "#/properties/responseBody/properties/data/items/properties/username",
                "type": "string",
                "title": "The Username Schema",
                "default": "",
                "examples": [
                  "ayucuis"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    },
    "resultType": {
      "$id": "#/properties/resultType",
      "type": "string",
      "title": "The Resulttype Schema",
      "default": "",
      "examples": [
        "employee"
      ],
      "pattern": "^(.*)$"
    },
    "requestBody": {
      "$id": "#/properties/requestBody",
      "type": "object",
      "title": "The Requestbody Schema",
      "required": [
        "parameters"
      ],
      "properties": {
        "parameters": {
          "$id": "#/properties/requestBody/properties/parameters",
          "type": "object",
          "title": "The Parameters Schema",
          "required": [
            "query"
          ],
          "properties": {
            "query": {
              "$id": "#/properties/requestBody/properties/parameters/properties/query",
              "type": "string",
              "title": "The Query Schema",
              "default": "",
              "examples": [
                "0096881"
              ],
              "pattern": "^(.*)$"
            }
          }
        }
      }
    }
  }
}